  echo -e $G"     J /      \.__\ $W"[$G--$W] $C"  Buscame en github: $R@Cesar-Hacker   $W[$G--$W]"
  echo -e $G"     J /      \.__\ $W"[$G--$W] $C"  Dracos Linux : $R@dracos-linux.org   $W[$G--$W]	"
